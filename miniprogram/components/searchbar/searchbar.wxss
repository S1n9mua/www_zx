.searchbar{
  background-color: #ADD8E6;
  padding: 15rpx;
}

.search-navigator{
  height: 60rpx;
  width: 100%;
  border-radius: 18rpx;
  background-color:  #79b6c9;
  /* background-color:  #0f4c75; */
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 6%;
}

.search-input-group{
  height: 60rpx;
  width: 100%;
  border-radius: 18rpx;
  background-color:  #79b6c9;
  /* background-color:  #0f4c75; */
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}

.search-input{
  min-height: 50rpx;
  height: 50rpx;
  font-size: 28rpx;
  color: #fff;
}

.search-input-group .placeholder-input{
  color: #fff;
}

