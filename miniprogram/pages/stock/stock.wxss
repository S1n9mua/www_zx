/* pages/stock/stock.wxss */

page {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  background: linear-gradient(#ADD8E6,#F0FFFF);
}

.container{
    padding: 50rpx 30rpx;
}

.add_device{
  background-color: #FFF;
  width: 90%;
  border-radius: 18rpx;
  padding: 30rpx;
  margin-bottom: 20rpx;
}

.add_title{
  margin:20rpx 30rpx;
  width: 160rpx;
  border-bottom:5rpx solid #0f4c75;
  color: rgb(27, 25, 25);
  font-size: 40rpx;
}

.add_input{
  padding: 20rpx 20rpx;
  /* 横向布局
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center; */
}

.add_input_hd{
  font-size: 28rpx;
  margin-bottom: 26rpx;
}

.add_input_bd{
  width: 96%;
  padding: 12rpx;
  transition: transform 250ms ease-in-out;
  font-size: 30rpx;
  line-height: 18rpx;
  color: #575756;
  background-color: transparent;
  /* background-image: url(http://mihaeltomic.com/codepen/input-search/ic_search_black_24px.svg); */
  background-repeat: no-repeat;
  background-size: 18rpx;
  background-position: 95% center;
  border-radius: 25rpx;
  border: 1px solid #575756;
  backface-visibility: hidden;
  transform-style: preserve-3d;
}

.add_input_bd .place-holder{
    color: rgba(87, 87, 86, 0.8);
    text-transform: uppercase;
    letter-spacing: 1.5px;
    /* padding-left:10rpx;  */
}

.image-list-group{
  padding-top: 20rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.image-list-group .image-add{
  margin-right: 5rpx;
  position: relative;
}
.image-list-group .image-add image{
  width: 100%;
  height: 100%;
}
.image-list-group .image-add .close-btn{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: -30rpx;
  top: -30rpx;
  z-index: 1;
  background-size: 50rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("data:image/png;base64,");
}

.image-list-group .add-image-btn{
  font-size: 200rpx;
  background: #f2f2f2;
  color: #a9a9a9;
  display: flex;
  justify-content: center;
  align-items: center;
}
.add-image-btn view{
  margin-top: -20rpx;
}

