/* miniprogram/pages/detail/detail.wxss */


page {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  background: linear-gradient(#add8e6, #f0ffff);
}

.container {
  padding: 50rpx 30rpx;
}

.add_device {
  background-color: #fff;
  width: 90%;
  border-radius: 18rpx;
  padding: 30rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.add_device .status-tap {
  position: absolute;
  width: 100rpx;
  height: 80rpx;
  right: 0;
  top: 10rpx;
  z-index: 1;
  color: white;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
  /* background-image: url(data:image/png;base64,); */
  /* /* 故障
  background-image: url(data:image/png;base64,); */

  
  background-image: url(data:image/png;base64,); 
}

.add_device .status-tap text {
  flex: 1;
  text-align: right;
  padding-right: 10rpx;
  padding-bottom: 5rpx;
}

.add_group {
  position: relative;
  margin-top: 20rpx;
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 30rpx;
}

.add_input {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  /* -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center; */
}

.add_input_hd {
  font-size: 28rpx;
  /* margin-bottom: 26rpx; */
  width: 150rpx;
  word-wrap: break-word;
  word-break: break-all;
  color: #a9a9a9;
}

.add_input_bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1；;
}

.btn-area {
  padding-top: 24rpx;
  padding-bottom: 24rpx;
  display: flex;
  justify-content: flex-end;
}


.btn-area button{
   width: 180rpx;
  padding-left: 16rpx;
  padding-right: 16rpx;
  border-radius: 50rpx;
  font-weight: 100rpx;
  font-size: 28rpx;
}

.btn-area .r-btn {
  background-color: #add8e6;
  color: #fff;
}

.btn-area .d-btn {
  background-color: #fff;
  color: #add8e6;
  border: 2rpx solid #add8e6;
}
